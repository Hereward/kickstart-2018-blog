{
  "parser": "babel-eslint",
  "parserOptions": {
    "allowImportExportEverywhere": true
  },
  
  "plugins": ["meteor", "prettier"],
  "extends": [
    "airbnb",
    "plugin:prettier/recommended",
    "plugin:meteor/recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "meteor": true,
    "mongo": true,
    "es6": true
  },
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "spaced-comment": "off",
    "new-cap": [
      1,
      { "capIsNewExceptions": ["Match", "Match.ObjectIncluding"] }
    ],
    "no-param-reassign": "warn",
    "no-underscore-dangle": [0],
    "no-extra-boolean-cast": [0],
    "no-return-assign": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx",".ts",".tsx"] }],
    "react/require-default-props": [0],
    "import/no-namespace": "off",
    "import/order": [
      2,
      {
        "groups": [
          ["builtin", "external"],
          ["internal", "parent", "sibling", "index"]
        ]
      }
    ],
    "import/newline-after-import": "off",
    "import/prefer-default-export": "off",
    "import/extensions": [0],
    "import/no-absolute-path": [0],
    "import/no-extraneous-dependencies": [0],
    "import/no-named-as-default": [0],
    "import/no-unresolved": [0],
    "no-use-before-define": "warn",
    "camelcase": "warn",
    "react/no-unescaped-entities": "warn",
    "class-methods-use-this": "off",
    "no-unused-vars": "warn",
    "no-else-return": "off",
    "no-undef": "warn",
    "consistent-return": "warn",
    "no-shadow": "off",
    "no-unneeded-ternary": "warn",
    "no-alert": "off",
    "react/jsx-indent": "off",
    "no-useless-constructor":"off",
    "react/no-string-refs":"warn",
    "react/jsx-boolean-value": "react/jsx-boolean-value",
    "react/prefer-stateless-function": "off",
    "react/sort-comp": "off",
    "react/no-typos": "warn",
    "lines-around-comment": "off",
    "prefer-template": "warn",
    "global-require": "warn",
    "react/no-unused-prop-types": "warn",
    "react/no-find-dom-node": "warn",
    "jsx-a11y/label-has-for": "warn",
    "jsx-a11y/anchor-is-valid": "off",
    "no-restricted-globals": "warn",
    "react/forbid-prop-types": "off",
    "react/prop-types": "warn",
    "arrow-body-style": "warn",
    "prefer-rest-params": "warn",
    "dot-notation": "off",
    "no-useless-escape": "off",
    "react/jsx-no-bind":"off",
    "meteor/no-session":"off",
    "vars-on-top": "warn",
    "one-var": "warn",
    "no-var": "warn",
    "no-plusplus": "warn",
    "func-names": "off",
    "eqeqeq": "warn",
    "radix": "warn",
    "strict": "warn",
    "no-redeclare": "warn",
    "block-scoped-var": "warn",
    "operator-assignment": "warn",
    "default-case": "off",
    "no-nested-ternary": "off",
    "no-extra-bind": "warn",
    "no-empty": "off",
    "react/no-unused-state": "react/no-unused-state",
    "react/jsx-closing-bracket-location": "off",
    "jsx-a11y/click-events-have-key-events": "off",
    "jsx-a11y/no-static-element-interactions": "warn",
    "no-unused-expressions": "warn",
    "semi": ["error", "always"],
    "no-lonely-if": "warn",
    "no-continue": "warn",
    "yoda": "warn",
    "react/jsx-first-prop-new-line": "off",
    "react/jsx-curly-brace-presence": "warn",
    "react/no-did-update-set-state": "off",
    "prefer-arrow-callback": ["off",
      {
        "allowNamedFunctions": true
      }
    ],
    "prefer-destructuring": [
      "off",
      {
        "array": true,
        "object": true
      },
      {
        "enforceForRenamedProperties": false
      }
    ],
    "prefer-const": [
      "warn",
      {
        "destructuring": "any",
        "ignoreReadBeforeAssign": false
      }
    ],
    "object-shorthand": ["off", "always", { "avoidQuotes": true }],
    "prettier/prettier": "off"
  }
}
